http {
	server_timeout_time       10000
	server {
		index                   index2.html
		server_name             bitch
		listen                  8082
		root                    www
		keepalive_timeout		10
		send_timeout			10
		max_body_size           100
		directory_listing       true
		location / {
			request_types       GET POST
		}
		location /pages {
			request_types       POST
		}
	}
	server {
		index                   index3.html
		server_name             127.0.0.1
